{
	"App": {
		"AuthServer": {
			"ApplicationCookieName": "simple-oidc-oauth-credentials",
			"BaseUrl": "https://localhost:5000",
			"IdentityProviderErrorPath": "/api/idp-error"
		},
		"Spa": {
			"BaseUrl": "http://localhost:3000",
			"LoginPath": "/login",
			"LogoutPath": "/logout"
		},
		"EmbeddedResources": {
			"ResourcesNamespace": "SimpleOidcOauth.EmbeddedResources"
		},
		"Email": {
			"Host": "localhost",
			"Port": 25,
			"DefaultSenderEmail": "no-reply@simple-oidc-oauth.com"
		},
		"Swagger": {
			"OpenApiDocumentRouteTemplate": "/api-docs/{documentName}/openapi.json",
			"SwaggerUIPageTitle": "simple-oidc-oauth: OpenAPI Documentation",
			"SwaggerUIRoutePrefix": "api-docs",
			"ApiTitleFull": "simple-oidc-oauth: auth-server",
			"ApiTitleShort": "auth-server",
			"ApiVersion": "v0.1-beta",
			"ApiDescription": "This documentation describes the REST API for interacting with the Identity Provider (IdP) Server which is part of the simple-oidc-oauth project.",
			"ApiDocumentName": "auth-server version 0.1 beta",
			"ApiDocumentNameUrlFriendly": "auth-server-v0.1-beta",
			"ApiContactName": "Vinicius Rog√©rio Araujo Silva",
			"ApiLicenseName": "Dummy License v2.0"
		}
	},
	"ConnectionStrings": {
		"IdentityServerConfigs": "Data Source=identity-database-configs.sqlite;",
		"IdentityServerOperational": "Data Source=identity-database-operational.sqlite;",
		"IdentityServerUsers": "Data Source=identity-database-users.sqlite;"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.EntityFrameworkCore.Database.Command": "Warning"
		}
	}
}